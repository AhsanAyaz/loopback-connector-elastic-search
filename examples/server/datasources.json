{
    "db": {
        "name": "db",
        "connector": "memory"
    },
    "elasticsearch-plain": {
        "name": "elasticsearch-plain",
        "connector": "elasticsearch",
        "index": "shakespeare",
        "hosts": [
            {
                "host": "hosted.foundcluster.com",
                "port": 9243
            }
        ],
        "log": "trace",
        "defaultSize": 50
    },
    "elasticsearch-ssl": {
      "name": "elasticsearch-ssl",
      "connector": "elasticsearch",
      "index": "shakespeare",
      "hosts": [
        {
          "protocol": "https",
          "host": "hosted.foundcluster.com",
          "port": 9243,
          "auth": "username:password"
        }
      ],
      "log": "trace",
      "defaultSize": 50,
      "ssl": {
        "ca": "./../cacert.pem",
        "rejectUnauthorized": true
      }
    }
}
